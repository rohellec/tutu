<%= form_for [:admin, railway_station] do |form| %>
  <%= render "common/errors", resource: railway_station %>

  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div class="actions">
    <%= f.submit railway_station.new_record? ? t(".create") : t(".update") %>
  </div>
<% end %>
